<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="info.mobilesgmc.home">

    <ImageView
        android:id="@+id/imgHomeActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/ic_launcher" />

    <TextView
        android:id="@+id/textView_HomeActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imgHomeActivity"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="@string/app_name"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/texto"
        android:textStyle="italic" />


    <TextView
        android:id="@+id/textViewCirurgia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="Teste"
        android:textColor="@color/texto"
        android:textStyle="italic" />

    <Button
        android:id="@+id/btnEscolhaCirurgia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="10dip"
        android:background="@drawable/buttonlogin"
        android:text="Escolha Cirurgia" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView_HomeActivity"
        android:layout_below="@+id/btnEscolhaCirurgia"
        android:layout_marginTop="42dp"
        android:text="Button"
        android:visibility="invisible" />


</FrameLayout>
